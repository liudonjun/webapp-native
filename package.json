{
  "name": "webapp-native",
  "version": "1.0.0",
  "description": "Web应用转原生应用构建系统 - 支持Android和iOS",
  "main": "index.js",
  "scripts": {
    "init": "node scripts/init.js",
    "build:web": "node scripts/build-web.js",
    "sync": "node scripts/sync-capacitor.js",
    "build:android": "node scripts/build-apk.js",
    "build:apk": "npm run build:web && npm run sync && npm run build:android",
    "build:ios": "node scripts/build-ios.js",
    "build:ipa": "npm run build:web && npm run sync && npm run build:ios",
    "generate:keystore": "node scripts/generate-keystore.js",
    "create:env": "node scripts/create-env.js",
    "create:env:example": "node scripts/create-env-example.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "dev": "cd web && npm run dev"
  },
  "keywords": [
    "capacitor",
    "android",
    "ios",
    "webapp",
    "native",
    "hybrid",
    "apk",
    "ipa",
    "build",
    "h5"
  ],
  "author": "liudonjun",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "vitepress": "^1.0.0"
  },
  "dependencies": {
    "@capacitor/core": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "@capacitor/ios": "^5.5.1"
  }
}

